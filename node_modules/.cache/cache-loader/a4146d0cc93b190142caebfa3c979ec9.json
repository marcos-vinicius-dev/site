fields[field].touched = true;\r\n                });\r\n            },\r\n            sendEmail() {\r\n                this.validateAll().then((result) => {\r\n                    if (result) {\r\n                        this.emailSendClicked = true;\r\n                        this.postToMailChimp();\r\n                    }\r\n                });\r\n            }\r\n        }\r\n    }\r\n</script>\r\n"]}]}